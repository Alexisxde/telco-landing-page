---
interface Props {
  width?: string | number
  height?: string | number
}

const { width = '16', height = '16' } = Astro.props
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  fill="none"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  stroke="currentColor">
  <path
    stroke-linecap="round"
    stroke-linejoin="round"
    d="M13.5 4.5 21 12m0 0-7.5 7.5M21 12H3"></path>
</svg>
