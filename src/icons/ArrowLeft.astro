---
interface Props {
  width?: string | number
  height?: string | number
}

const { width = '16', height = '16' } = Astro.props
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  fill="none"
  viewBox="0 0 24 24"
  stroke-width="1.5"
  stroke="currentColor">
  <path
    stroke-linecap="round"
    stroke-linejoin="round"
    d="M10.5 19.5 3 12m0 0 7.5-7.5M3 12h18"></path>
</svg>
